{
  "name": "midas-desktop",
  "title": "Midas-desktop",
  "description": "Midas-desktop",
  "version": "1.0.0",
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "build:start:dev": "npm run build:dev && npx electron .",
    "build:start:prod": "npm run build:dev && npx electron .",
    "start:built": "npx electron .",
    "build-electron": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --base-href ./ --outputPath=./dist",
    "build-package": "npm run build-electron --prefix .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=dist/assets/favicon.icns --prune=true --out=release-builds/mac",
    "package-win32": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=dist/assets/favicon.ico --prune=true --out=release-builds/win32 --version-string.CompanyName='midas-desktop' --version-string.FileDescription='midas-desktop' --version-string.ProductName='midas-desktop'",
    "package-win64": "electron-packager . --overwrite --asar --platform=win32 --arch=x64 --icon=dist/assets/favicon.ico --prune=true --out=release-builds/win64 --version-string.CompanyName='midas-desktop' --version-string.FileDescription='midas-desktop' --version-string.ProductName='midas-desktop'",
    "installer-win64": "node build_installer64.ts",
    "package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --icon=dist/assets/icons/favicon.png --prune=true --out=release-builds/linux",
    "installer-mac": "electron-installer-dmg ./release-builds/mac/midas-desktop-darwin-x64/midas-desktop.app midas-desktop --out=release-builds/mac/installer --overwrite --icon=dist/assets/favicon.icns",
    "complete-win64": "npm run build-package && npm run package-win64 && npm run installer-win64",
    "br": "npm run electron:build && npx electron ."
  },
  "dependencies": {
    "@electron/remote": "1.0.4",
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow/tfjs": "^4.2.0",
    "@types/electron-json-storage": "^4.5.0",
    "electron-json-storage": "^4.5.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.3.1",
    "jszip": "^3.6.0",
    "long": "^5.2.1",
    "objects-to-csv": "^1.3.6"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.1.1",
    "@angular-devkit/build-angular": "0.1102.5",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/animations": "10.1.1",
    "@angular/cdk": "^11.2.5",
    "@angular/cli": "11.2.5",
    "@angular/common": "11.2.6",
    "@angular/compiler": "11.2.6",
    "@angular/compiler-cli": "11.2.6",
    "@angular/core": "11.2.6",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.2.6",
    "@angular/language-service": "11.2.6",
    "@angular/material": "11.0.0",
    "@angular/platform-browser": "11.2.6",
    "@angular/platform-browser-dynamic": "11.2.6",
    "@angular/router": "11.2.6",
    "@auth0/angular-jwt": "4.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@schematics/angular": "^11.2.9",
    "@tensorflow/tfjs": "^4.2.0",
    "@types/file-saver": "^2.0.2",
    "@types/jasmine": "3.6.7",
    "@types/jasminewd2": "2.0.8",
