    "hammerjs": "2.0.8",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "6.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lodash": "^4.17.21",
    "mocha": "8.3.2",
    "moment": "2.29.1",
    "npm-run-all": "4.1.5",
    "protractor": "7.0.0",
    "rxjs": "6.6.6",
    "spectron": "14.0.0",
    "ts-loader": "8.0.3",
    "ts-node": "9.1.1",
